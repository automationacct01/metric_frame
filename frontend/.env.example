# Environment Configuration Template for NIST CSF 2.0 Frontend
# Copy this file to .env.development for development settings

# Backend API Base URL
# For development: http://localhost:8000/api/v1
# For production: https://your-api-domain.com/api/v1
VITE_API_BASE_URL=http://localhost:8000/api/v1

# Environment mode
VITE_NODE_ENV=development

# Enable debug logging
VITE_DEBUG=true